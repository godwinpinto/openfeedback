# Chrome Origin Trial Tokens
# These tokens enable Chrome's experimental APIs (Prompt, Writer, Rewriter, Proofreader) in your domain.
# Get your tokens from: https://developer.chrome.com/origintrials/
# Each token is specific to your domain and allows access to the corresponding Chrome API.
PROMPT_API_ORIGIN_TRIAL_TOKEN=<ORIGIN_TRIAL_KEY_FOR_YOUR_DOMAIN_FROM_CHROME>
WRITER_API_ORIGIN_TRIAL_TOKEN=<YOUR_WRITER_API_ORIGIN_TRIAL_TOKEN>
REWRITER_API_ORIGIN_TRIAL_TOKEN=<YOUR_REWRITER_API_ORIGIN_TRIAL_TOKEN>
PROOFREADER_API_ORIGIN_TRIAL_TOKEN=<YOUR_PROOFREADER_API_ORIGIN_TRIAL_TOKEN>

# Database Configuration
# PostgreSQL connection string for Drizzle ORM
# Format: postgresql://[user]:[password]@[host]:[port]/[database]
DATABASE_URL=postgresql://user:password@localhost:5432/openfeedback

# Google Generative AI
# API key for Google's Gemini models (used for remote chat transport)
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_GENERATIVE_AI_API_KEY=<YOUR_GOOGLE_GENERATIVE_AI_API_KEY>

# Public Configuration
# URL for the "Get Started" link in the header (public, accessible in the browser)
# Leave empty to use the default "/" route
NEXT_PUBLIC_GET_STARTED_URL=
